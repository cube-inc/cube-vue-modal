{
  "name": "cube-vue-modal",
  "version": "0.1.2",
  "description": "",
  "author": "Fred Gomez <fred@joincube.ca>",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/cube-inc/cube-vue-modal.git"
  },
  "keywords": [
    "vue",
    "modal"
  ],
  "main": "src/components/CubeModal.vue",
  "_main": "dist/cube-vue-modal.umd.js",
  "_module": "dist/cube-vue-modal.esm.js",
  "_unpkg": "dist/cube-vue-modal.min.js",
  "browser": {
    "./sfc": "src/components/CubeModal.vue"
  },
  "files": [
    "dist",
    "src/components/CubeModal.vue"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "cli:build": "vue-cli-service build --target lib --name cube-modal src/components/CubeModal.vue",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/cube-vue-modal.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/cube-vue-modal.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/cube-vue-modal.min.js",
    "prepublish": "yarn run lint && yarn run test:unit && yarn run build"
  },
  "dependencies": {
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-plugin-unit-jest": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "@vue/eslint-config-standard": "^3.0.5",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.0.1",
    "node-sass": "^4.9.0",
    "rollup": "^0.57.1",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-vue": "^3.0.0",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.5.17"
  }
}
